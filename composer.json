{
    "name": "stormpath/wordpress",
    "description": "Allow the use of users from Stormpath to login into your WordPress site.  Creates a user in stormpath if they only exist on WordPress.",
    "type": "library",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Brian Retterer",
            "email": "brian@stormpath.com"
        }
    ],
    "require-dev": {
        "stevegrunwell/wp-enforcer": "^0.4.1",
        "phpunit/phpunit": "^5.5",
        "10up/wp_mock": "^0.1.1"
    },
    "post-install-cmd": [
        "wp-enforcer --vip"
    ],
    "post-update-cmd": [
        "wp-enforcer --vip"
    ],
    "require": {
        "symfony/yaml": "^3.1",
        "stormpath/sdk": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Stormpath\\WordPress\\": "lib"
        }
    }
}
